{
  "name": "mars-app",
  "description": "Mars app",
  "author": "Simonas Karuzas <simonas@not.cat>",
  "version": "0.1.16",
  "private": true,
  "main": "build/electron.js",
  "build": {
    "appId": "io.veramolabs.mars",
    "mac": {
      "category": "your.app.category.type"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*",
      "electron/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "dependencies": {
    "@auth0/auth0-spa-js": "^1.7.0",
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.3.2",
    "@testing-library/user-event": "^7.1.2",
    "@types/jest": "^24.0.0",
    "@types/node": "^12.0.0",
    "@types/react": "^16.9.0",
    "@types/react-dom": "^16.9.0",
    "@types/react-router-dom": "^5.1.5",
    "daf-cli": "^7.0.0-beta.57",
    "daf-core": "^7.0.0-beta.57",
    "daf-did-comm": "^7.0.0-beta.57",
    "daf-did-jwt": "^7.0.0-beta.57",
    "daf-message-handler": "^7.0.0-beta.57",
    "daf-resolver": "^7.0.0-beta.57",
    "daf-rest": "^7.0.0-beta.57",
    "daf-selective-disclosure": "^7.0.0-beta.57",
    "daf-typeorm": "^7.0.0-beta.57",
    "daf-w3c": "^7.0.0-beta.57",
    "date-fns": "^2.12.0",
    "did-resolver": "^2.1.2",
    "ethr-did-resolver": "^3.0.2",
    "get-port": "^5.1.1",
    "graphql": "^15.0.0",
    "material-ui-dropzone": "^3.5.0",
    "notistack": "^1.0.2",
    "prettier": "^2.2.1",
    "qrcode-react": "^0.1.16",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-qr-reader": "^2.2.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.4.1",
    "serve": "^11.3.2",
    "shortid": "^2.2.15",
    "typescript": "~3.7.2",
    "url-parse": "^1.4.7",
    "web-did-resolver": "^1.3.5"
  },
  "scripts": {
    "start": "serve -s build -p $PORT",
    "dev": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "upgrade-daf": "yarn add daf-core@beta daf-w3c@beta daf-typeorm@beta daf-rest@beta daf-cli@beta daf-w3c@beta daf-selective-disclosure@beta daf-did-jwt@beta daf-did-comm@beta daf-message-handler@beta daf-resolver@beta",
    "build-electron": "cross-env PUBLIC_URL=./ react-scripts build",
    "electron": "electron .",
    "electron-dev": "cross-env ELECTRON_START_URL=http://localhost:3000 electron .",
    "electron-dist": "electron-builder"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/shortid": "^0.0.29",
    "@types/url-parse": "^1.4.3",
    "cross-env": "^7.0.3",
    "electron": "^11.0.3",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.4",
    "husky": "^4.3.5",
    "lint-staged": "^10.5.3"
  }
}
